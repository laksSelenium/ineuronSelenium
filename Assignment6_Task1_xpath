//Login button
//Xpath
1. //button[text()=' Login ']
2. //button[@type='submit']
3. //button[contains(@type,'submit')]
4. //button[contains(@class,'oxd-button')]
5. //div[contains(@class,'login-action')]//button
6. //div[contains(@class,'login-form')]//button
7. //div[contains(@class,'login-form')]//following::button
8. //div[contains(@class,'forgot')]//preceding::button
9.//div[contains(@class,'forgot')]//..//following::button
10. //div[contains(@class,'forgot')]//parent::div//preceding::button

//CSS
1. button[type='submit']
2. .oxd-button
3. button.oxd-button
4. .oxd-form button
5. .oxd-form .oxd-form-actions button
6. .orangehrm-login-form button

