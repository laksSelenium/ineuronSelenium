//username:
//By Xpath:

1. //input[@name='username']
2. //input[@placeholder='Username']
3. //input[contains(@name,'username')]
4. //input[contains(@name,'name')]
4. //input[contains(@placeholder,'name')]
5. //input[contains(@placeholder,'User')]
6. //div[contains(@class,'login-form')]//input[@name='username']
7. (//div[contains(@class,'login-form')]//div[2]//input)[1]
8. //i[contains(@class,'bi-person')]//..//..//input
9. //label[text()='Username']//..//..//input
10. (//label[text()='Password']//preceding::input)[2]
11. //label[text()='Password']//preceding::input[contains(@name,'username')]
12. //label[text()='Username']//following::input[contains(@name,'username')]
13. (//label[text()='Username']//following::input)[1]
14. //label[text()='Username']//parent::div//parent::div//input
15. //input[@name='username' and @placeholder='Username']
16. //input[@name='username' or @placeholder='Username']

//username:
//By CSS:
1. input[name='username']
2. input[placeholder='Username']
3. div.oxd-input-group input:first-of-type[name='username']
4. .orangehrm-login-form div.oxd-form-row:first-of-type input
5. .orangehrm-login-form div.oxd-form-row:nth-of-type(1) input
6. .orangehrm-login-form div:nth-child(2) input:first-of-type[name='username']
7. input[name*='username']
8. input[name^='user']
9. input[name$='name']
10. input[placeholder$='name']
