//Password
//By.xpath
1. //input[@name='password']
2. //input[@placeholder='Password']
3. //input[@type='password']
4. //input[contains(@type,'password')]
5. //input[contains(@type,'password') and @name='password']
6. //input[contains(@type,'password') or @name='password']
7. //i[contains(@class,'bi-key')]//..//..//input
8. //form[@class='oxd-form']//input[@name='password']
9. (//div[@class='oxd-form-row'])[2]//input
10. (//div[@class='oxd-form-row'])[2]//following::input
11. (//i[contains(@class,'bi-key')]//preceding::div//following::input)[3]
12. //label[text()='Password']//preceding::div[@class='oxd-form-row']//input
13. //i[contains(@class,'bi-key')]//..//following-sibling::div//input
14. //label[text()='Password']//following::input
15. (//div[@class='oxd-input-group__label-wrapper']//following-sibling::div)[2]//input

//Password
//By CSS
1. input[type='password']
2. input[name='password']
3. input[name*='password']
4. input[name^='pass']
5. input[name$='word']
6. .orangehrm-login-form div.oxd-form-row:nth-of-type(2) input
7. .oxd-input-group div:nth-of-type(2) input[name='password']
8. .orangehrm-login-form div.oxd-form-row:nth-of-type(2) input
9. div.oxd-input-group input:first-of-type[name='password']
10. input[placeholder$='word']

